<Window x:Class="BabyOpgave.MainWindow"
        Loaded="MainWindow_OnLoaded"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BabyOpgave"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <ListBox x:Name="listBox" DisplayMemberPath="Name" HorizontalAlignment="Left" Height="175" Margin="123,30,0,0" VerticalAlignment="Top" Width="369" SelectionChanged="listBox_SelectionChanged"/>

        <ListBox x:Name="listBox1" HorizontalAlignment="Left" Height="175" Margin="10,30,0,0" VerticalAlignment="Top" Width="72" SelectionChanged="listBox1_SelectionChanged"/>
        <Label x:Name="label" Content="Top 10 baby names in selected decade" HorizontalAlignment="Left" Margin="123,4,0,0" VerticalAlignment="Top" Width="318"/>
        <Label x:Name="label1" Content="Decade" HorizontalAlignment="Left" Margin="10,4,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
