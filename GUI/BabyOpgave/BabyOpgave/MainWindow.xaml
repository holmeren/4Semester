<Window x:Class="BabyOpgave.MainWindow"
        Loaded="MainWindow_OnLoaded"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BabyOpgave"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <ListBox x:Name="listBox" DisplayMemberPath="Name" HorizontalAlignment="Left" Height="175" Margin="180,10,0,0" VerticalAlignment="Top" Width="235" SelectionChanged="listBox_SelectionChanged"/>
        
        <ListBox x:Name="listBox1" HorizontalAlignment="Left" Height="195" Margin="10,10,0,0" VerticalAlignment="Top" Width="100" SelectionChanged="listBox1_SelectionChanged"/>

    </Grid>
</Window>
